{
  "datasets": [
    {
      "name": "bffc80bc",
      "displayName": "Orders & Revenue Over Time",
      "queryLines": [
        "SELECT\n",
        "  order_date,\n",
        "  orders_count,\n",
        "  revenue,\n",
        "  items_subtotal,\n",
        "  items_sold,\n",
        "  avg_order_value\n",
        "FROM olist.gold.orders_daily\n",
        "WHERE order_date IS NOT NULL\n",
        "ORDER BY order_date;\n"
      ],
      "catalog": "olist",
      "schema": "gold"
    },
    {
      "name": "dff580e8",
      "displayName": "Revenue by Category",
      "queryLines": [
        "SELECT\n",
        "  category AS category_name,\n",
        "  items_subtotal,\n",
        "  orders_count,\n",
        "  items_sold\n",
        "FROM olist.gold.revenue_by_category\n",
        "WHERE category IS NOT NULL\n",
        "ORDER BY items_subtotal DESC\n",
        "LIMIT 15;\n"
      ]
    },
    {
      "name": "68894bf2",
      "displayName": "Revenue by State",
      "queryLines": [
        "SELECT\n",
        "  customer_state,\n",
        "  revenue,\n",
        "  orders_count\n",
        "FROM olist.gold.revenue_by_state\n",
        "ORDER BY revenue DESC;\n"
      ]
    },
    {
      "name": "7fa7140b",
      "displayName": "Delivery Performance (Monthly)",
      "queryLines": [
        "SELECT\n",
        "  order_month,\n",
        "  avg_delivery_days,\n",
        "  pct_delivered_on_time\n",
        "FROM olist.gold.delivery_monthly\n",
        "ORDER BY order_month;\n"
      ]
    },
    {
      "name": "66f2edb8",
      "displayName": "Payment Type Mix",
      "queryLines": [
        "SELECT\n",
        "  payment_type,\n",
        "  orders_count,\n",
        "  revenue\n",
        "FROM olist.gold.payment_type_mix\n",
        "ORDER BY orders_count DESC;\n"
      ]
    },
    {
      "name": "6dca2b20",
      "displayName": "Top Sellers",
      "queryLines": [
        "SELECT\n",
        "  seller_id,\n",
        "  seller_state,\n",
        "  items_subtotal,\n",
        "  orders_count\n",
        "FROM olist.gold.top_sellers\n",
        "ORDER BY items_subtotal DESC\n",
        "LIMIT 10;\n"
      ]
    }
  ],
